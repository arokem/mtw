# Configuration for coverage.py

[run]
branch = True
source = mtw
include = */mtw/*
omit = */setup.py
       */mtw/examples_utils/*
       */mtw/plot_utils.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
